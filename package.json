{
    "name": "simple-calculator-logic",
    "version": "0.0.0",
    "description": "A simple Calculator object",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": "git@github.com:angelxmoreno/simple-calculator-logic.git",
    "author": "Angel S. Moreno <angelxmoreno@gmail.com>",
    "license": "MIT",
    "private": false,
    "scripts": {
        "prepublish": "tsc",
        "test": "mocha -r ts-node/register tests/**/*.spec.ts",
        "test:coverage": "nyc yarn run test",
        "lint": "eslint . --cache",
        "lint:fix": "eslint . --cache --fix",
        "prepare": "husky install",
        "lint-staged": "lint-staged",
        "release": "standard-version",
        "release:minor": "standard-version --release-as minor",
        "release:patch": "standard-version --release-as patch",
        "release:major": "standard-version --release-as major"
    },
    "dependencies": {},
    "devDependencies": {
        "@commitlint/cli": "18.2.0",
        "@commitlint/config-conventional": "18.1.0",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/chai": "^4.3.9",
        "@types/mocha": "^10.0.3",
        "@types/node": "^20.8.10",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "@typescript-eslint/parser": "^6.9.1",
        "chai": "^4.3.10",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^9.0.0",
        "eslint-config-standard-with-typescript": "^39.1.1",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-markdownlint": "^0.5.0",
        "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-promise": "^6.0.0",
        "husky": "^8.0.3",
        "lint-staged": "^15.0.2",
        "mocha": "^10.2.0",
        "nyc": "^15.1.0",
        "prettier": "^3.0.3",
        "standard-version": "^9.5.0",
        "ts-node": "^10.9.1",
        "typescript": "*"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "files": [
        "dist/**",
        "LICENSE",
        "README.md",
        "CHANGELOG.md"
    ]
}
